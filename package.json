{
  "name": "praetbot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "tsx app.ts",
    "dev:watch": "tsx watch app.ts",
    "dev:web": "tsx bin/www.ts",
    "dev:web:watch": "tsx watch bin/www.ts",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md,yml,yaml}\""
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@types/node": "^22.10.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "debug": "^4.3.7",
    "discord.js": "^14.16.3",
    "express": "^4.21.2",
    "hbs": "^4.2.0",
    "mongodb": "^6.11.0",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0",
    "vm2": "^3.9.19"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/serve-favicon": "^2.5.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
