{
  "name": "praetbot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:live & npm run build:webLive",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./app.ts",
    "build:webLive": "nodemon --exec ./node_modules/.bin/ts-node -- ./bin/www",
    "build:prod": "./node_modules/.bin/ts-node ./app.ts",
    "build:web": "./node_modules/.bin/ts-node ./bin/www"
  },
  "engines": {
    "node": "10.15.0"
  },
  "dependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.48",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.1.1",
    "mongodb": "^2.2.11",
    "morgan": "~1.7.0",
    "node-fetch": "^1.6.3",
    "nodemon": "^1.14.11",
    "serve-favicon": "~2.3.0",
    "slack": "^8.1.0",
    "slackbots": "^0.5.3",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10"
  }
}
